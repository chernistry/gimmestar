{
  "name": "gimmestar",
  "version": "0.1.0",
  "description": "GitHub Star Exchange - A platform for organic repository discovery and engagement",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && next build",
    "build:backend": "tsc",
    "build:frontend": "next build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "dev": "tsx watch src/index.ts",
    "dev:frontend": "next dev -p 3001",
    "start:frontend": "next start -p 3001",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "github",
    "stars",
    "exchange"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.5.6",
    "pg": "^8.16.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.9.3",
    "@types/pg": "^8.15.6",
    "@vitest/ui": "^2.1.8",
    "drizzle-kit": "^0.31.7",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "tsx": "^4.19.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
